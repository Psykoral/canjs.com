<div class="tab-description github">
  <div class="pull-right">
    <a href="http://github.com/bitovi/canjs" class="button"><span class="icon-github"></span>Follow on Github</a>
  </div><!-- categories -->
  <div class="posts">

    {{#commits}}
    <div class="bithub-content">
      <h2><a href="{{link}}">{{actor}} {{title}}</a></h2>
      {{#commits}}
      <p>- {{truncateHash hash}} {{message}}</p>
      {{/commits}}
      <div class="bithub-footer">{{#if actor}}<a href="http://bithub.com/users/{{actorID}}">{{actor}}</a> / {{/if}}{{formatDate date}} via <a href="{{link}}">{{feed}}</a></div>
    </div>
    {{/commits}}
  </div>
  <br class="clear" />
</div>